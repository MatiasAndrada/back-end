{{#if listExist}}
  <ul class="list-group list__product">
    {{#each list}}
      <li class="list__item">
 
          <img class="list__item--img" src="{{this.thumbnail}}" alt="{{this.title}}">

        <h3 class="list__item--title">{{this.name}}</h3>
        <p class="list__item--total">${{this.price}}</p>
      </li>
    {{/each}}
  </ul>
{{/if}}